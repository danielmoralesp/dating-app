$(document).ready(function() {
	$(".hide-match").on("ajax:complete", function(event) {
		$(event.currentTarget).parent().parent().hide();
	});

});

$(document).on('page:load', function(event) {
	$(".hide-match").on("ajax:complete", function(event) {
		$(event.currentTarget).parent().parent().hide();
	});

});
